<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">


    <bean id="empbean1" class="com.stackroute.domain.Employee">

        <property name="empId" value="101"></property>
        <property name="empName" value="John"></property>
    </bean>

    <bean id="empbean2" class="com.stackroute.domain.Employee">

        <property name="empId" value="102"></property>
        <property name="empName" value="Jenny"></property>
    </bean>

    <bean id="empbean3" class="com.stackroute.domain.Employee">

        <constructor-arg value="103"></constructor-arg>
        <constructor-arg value="Joshnson"></constructor-arg>
    </bean>



</beans>